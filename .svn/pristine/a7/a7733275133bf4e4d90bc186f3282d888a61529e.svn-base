<UserControl x:Class="Hdc.Mv.PropertyItem.Controls.CircleControlEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             >
    <StackPanel Orientation="Vertical">
        <Border Background="Lavender">
            <RadioButton BorderThickness="1" BorderBrush="Blue" x:Name="RadioButtonCircle" GroupName="DrawObjectType" Checked="RadioButton_OnChecked">Circle</RadioButton>
        </Border>
    </StackPanel>
</UserControl>
