using System;

namespace Hdc.Mv.Inspection
{
    public class DefectInspectionException: Exception
    {
        public DefectInspectionException()
        {
        }

        public DefectInspectionException(string message) : base(message)
        {
        }

        public DefectInspectionException(string message, Exception innerException) : base(message, innerException)
        {
        }
    }
}