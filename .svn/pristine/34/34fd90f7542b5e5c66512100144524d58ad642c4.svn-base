using System.Windows.Media;

namespace Hdc.Mv.Mvvm
{
    public class CircleIndicatorViewModel1
    {
        public double CenterX { get; set; }
        public double CenterY { get; set; }
        public double Radius { get; set; }
        public bool DisplayEnabled { get; set; } = true;

        public CircleIndicatorViewModel1()
        {
        }

        public CircleIndicatorViewModel1(double centerX, double centerY, double radius)
        {
            CenterX = centerX;
            CenterY = centerY;
            Radius = radius;
        }
    }
}